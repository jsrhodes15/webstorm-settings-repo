    </context>
  </template>
  <template name="enz" value="import React from 'react';&#10;import { mount } from 'enzyme';&#10;&#10;describe('$NAME$', () =&gt; {&#10;  $END$&#10;});&#10;" description="Enzyme test file" toReformat="false" toShortenFQNames="true">
